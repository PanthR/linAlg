   "main": "index.js",
   "scripts": {
      "test": "mocha --reporter=spec --recursive --require=./test/stackFixer.js",
      "doc": "node generateDoc.js"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/PanthR/linAlg.git"
   },
   "keywords": [
   "matrices",
   "linear-algebra",
   "vectors",
   "linear-solvers",
