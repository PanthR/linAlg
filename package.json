   "bugs": {
      "url": "https://github.com/PanthR/linAlg/issues"
   },
   "homepage": "https://github.com/PanthR/linAlg",
   "devDependencies": {
      "chai": "^1.9.1",
      "handlebars": "^2.0.0-alpha.4",
      "mocha": "~1.21.3",
      "mocha-jshint": "0.0.9"
   },
   "jshintConfig": {
      "bitwise": true,
      "curly": true,
      "camelcase": true,
      "eqeqeq": true,
